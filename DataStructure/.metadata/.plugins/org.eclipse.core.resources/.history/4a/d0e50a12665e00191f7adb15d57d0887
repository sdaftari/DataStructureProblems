package Easy;


public class MoveZerosToEnd {

	public static void main(String[] args) {
		int[] nums = {0, 1, 0, 3, 12};
		moveZeroes(nums);
		for (int i : nums)
			System.out.print(i + " ");
	}

	public static void moveZeroes(int[] nums) {
        if (nums == null || nums.length == 0)
            return;
        
        int index = 0;
        
        for (int n : nums)
        {
            if (n != 0)
                nums[index++] = n;
        }
        
        while (index < nums.length)
                nums[index++] = 0;
    }
}
