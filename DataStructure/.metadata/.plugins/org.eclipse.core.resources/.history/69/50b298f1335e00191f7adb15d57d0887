package Medium;

public class FlipBits {
	
	private int flipBits(int[] arr)
	{
		if (arr == null || arr.length == 0)
			return 0;
		
		int countOne = 0;
		for (int i = 0; i < arr.length; i++)
			if (arr[i] == 1)
				countOne++;
		for (int i = 0; i < arr.length; i++)
		{
			if (arr[i] == 0)
				arr[i] = 1;
			else 
				arr[i] = -1;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
