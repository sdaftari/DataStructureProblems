package Easy;

public class ValueEqualToIndex {
	
	// Unsorted array
	static int linearSearch(int arr[], int n) 
    { 
        int i; 
        for(i = 0; i < n; i++) 
        { 
            if(arr[i] == i) 
                return i; 
        } 
        
        /* If no fixed point present  
           then return -1 */
        return -1; 
    } 

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
